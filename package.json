{
  "name": "nextjs-starter",
  "version": "1.0.0",
  "private": true,
  "description": "Template for nextjs + typescript",
  "homepage": "https://github.com/zlfnhmd/next-typescript#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zlfnhmd/next-typescript.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zlfnhmd/next-typescript/issues"
  },
  "author": "Zulfan Hameed <zlfnhmd@gmail.com>",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "post-merge-install",
      "post-rebase": "post-merge-install"
    }
  },
  "lint-staged": {
    "**/*.{json}": [
      "prettier --write"
    ],
    "**/*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "next": "^9.5.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@cleartax/eslint-config": "^1.3.2",
    "@types/classnames": "^2.2.10",
    "@types/eslint": "^6.8.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^14.14.2",
    "@types/prettier": "^1.19.1",
    "@types/react": "^16.9.53",
    "@types/react-dom": "16.9.8",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-markdown": "^1.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "post-merge-install": "^0.4.0",
    "prettier": "^1.19.1",
    "typescript": "^4.0.5"
  }
}
